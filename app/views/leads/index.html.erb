<h1>Leads</h1>

<div class="table-responsive">
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>Updated at</th>
        <th>Type</th>
        <th>Source</th>
        <th>Product id</th>
        <th>Product name</th>
      </tr>
    </thead>

    <tbody>
      <% @leads.each do |lead| %>
      <tr>
        <td><%= lead.updated_at %></td>
        <td><%= lead.lead_type %></td>
        <td><%= lead.lead_source %></td>
        <td><%= lead.product&.id %></td>
        <td><%= lead.product&.name %></td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
